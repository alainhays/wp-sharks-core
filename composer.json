{
  "name": "websharks/wp-sharks-core",
  "homepage": "https://github.com/websharks/wp-sharks-core",
  "description": "Core WP plugin framework used by WP Sharksâ„¢",
  "keywords": ["websharks", "wp sharks", "wordpress"],

  "type": "library",
  "license": "GPL-3.0+",

  "authors": [{
    "name": "websharks",
    "homepage": "http://websharks-inc.com/",
    "role": "company"
  }, {
    "name": "jaswsinc",
    "homepage": "http://jaswsinc.com/",
    "role": "developer"
  }, {
    "name": "raamdev",
    "homepage": "http://raam.org/",
    "role": "developer"
  }],

  "support": {
    "source": "https://github.com/websharks/wp-sharks-core",
    "issues": "https://github.com/websharks/wp-sharks-core/issues"
  },

  "require": {
    "websharks/core": "dev-master",
    "websharks/wp-php-rv": "dev-master"
  },
  "require-dev": {
    "package/bourbon": "4.2.3"
  },

  "minimum-stability": "dev",
  "prefer-stable": true,

  "autoload": {
    "psr-4": {
      "WebSharks\\WpSharks\\Core\\Classes\\": "src/includes/classes",
      "WebSharks\\WpSharks\\Core\\Functions\\": "src/includes/functions",
      "WebSharks\\WpSharks\\Core\\Interfaces\\": "src/includes/interfaces",
      "WebSharks\\WpSharks\\Core\\Traits\\": "src/includes/traits"
    }
  },

  "config": {
    "vendor-dir": "src/vendor",
    "preferred-install": "dist"
  },

  "repositories": [{
    "type": "package",
    "package": {
      "name": "package/bourbon",
      "version": "4.2.3",
      "dist": {
        "url": "https://github.com/thoughtbot/bourbon/releases/download/v4.2.3/bourbon-v4.2.3.zip",
        "type": "zip"
      }
    }
  }]
}
